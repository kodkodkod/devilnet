<!-- Page Content -->
<div class="container">

    <div class="row">




        <div class="col-md-12">


            <div class="row">
                <% products.forEach(function(item) { %>
                <div>

                    <div class="col-sm-4 col-lg-4 col-md-4">
                        <a class="thumbnail" href="/product/id/<%= item.id %>" >
                            <img src="<%= item.url %>" alt="">
                            <div class="caption">
                                <h4 class="pull-right"><%= item.price %> ZomboCoins</h4>
                                <h4><%= item.name %></h4>
                                <p><%= item.description %></p>
                            </div>
                            <%if (item.level > 0) { %>
                            <div class="ratings">
                                <p class="pull-right">Fear lever <%= item.level %></p>
                                <p>
                                    <% for(var i = 0; i < item.level; i++) {%>
                                    <span class="glyphicon glyphicon-star"></span>
                                    <% } %>
                                </p>
                            </div>
                            <% } %>
                        </a>
                    </div>
                </div>

                <% }); %>


            </div>

        </div>

    </div>

</div>
<!-- /.container -->